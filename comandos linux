./nome do executavel

criar pasta 
mkdir nome da pasta

reiniciar rede 

sudo service network-manager restart

reiniciar bluetooth

sudo service bluetooth restart

sudo service xboxdrv restart

	Comandos Git

git init // inicializa repositorio 

	apagando repositorio
cd pasta do gite
rm -rf .git
git status// vetifica o status do git

	adicionar arquivo no git 
git add nome do arquivo
	colocar todos os arquivos

git add .
	atualizando arquivo

git commit -m "mensagem"
//gravaçao e feita localmente 

modified == moificado 

staged == marcou um rquivo modificado 

	retornar a versao anterior do documento

git checkout  ID do document
 atualizaçao
  





